{
  "name": "portoflio",
  "version": "2.0.1",
  "description": "My personal website.",
  "repository": "https://github.com/mattcarlotta/portfolio",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": false,
  "scripts": {
    "analyze:prod": "rimraf .next/analyze && analyze=true npm run build",
    "analyze:stage": "rimraf .next/analyze && analyze=true npm run build:stage",
    "build": "ENV_DEBUG=true ENV_LOAD=.env.prod inStaging=false NODE_OPTIONS='-r snackables' next build",
    "build:stage": "ENV_DEBUG=true ENV_LOAD=.env.prod inStaging=true NODE_OPTIONS='-r snackables' next build",
    "dev": "ENV_CACHE=true ENV_DEBUG=true ENV_LOAD=.env.dev NODE_OPTIONS='--max_old_space_size=4096 --inspect -r snackables' next dev",
    "export": "next export",
    "lint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./src --ext .ts,.tsx",
    "stage": "ENV_DEBUG=true ENV_LOAD=.env.prod NODE_OPTIONS='-r snackables' inStaging=true next start -p 8080",
    "start": "ENV_DEBUG=true ENV_LOAD=.env.prod NODE_OPTIONS='-r snackables' next start -p 8080",
    "test": "jest --config ./jest.json --passWithNoTests",
    "test:cov": "jest --config ./jest.json --coverage",
    "test:e2e": "cypress open --project ./e2e",
    "test:e2erun": "npm run build:stage && concurrently --success first --kill-others \"npm run start\" \"cypress run --config video=false --project ./e2e\"",
    "test:failed": "jest --config ./jest.json --watch --onlyFailures",
    "test:prod": "npm run build:prod && npm run start",
    "test:stage": "npm run build:stage && npm run stage",
    "test:watch": "jest --config ./jest.json --watch --onlyChanged",
    "test:watchall": "jest --config ./jest.json .-watchAll"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint",
      "npm run test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "chalk": "^4.1.0",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "next": "^10.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "snackables": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@emotion/jest": "^11.2.1",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.22",
    "@types/jsdom": "^16.2.10",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isempty": "^4.4.6",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "address": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "browserslist": "^4.16.3",
    "concurrently": "^6.0.0",
    "cypress": "^6.8.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-watch": "^7.0.0",
    "file-loader": "^6.2.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-css-modules-transform": "^4.2.0",
    "jest-emotion": "^11.0.0",
    "jest-enzyme": "^7.1.2",
    "jest-haste-map": "^26.6.2",
    "jest-resolve": "^26.6.2",
    "jsdom": "^16.5.2",
    "lint-staged": "^10.5.4",
    "opener": "^1.5.2",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.30.0",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
