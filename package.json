{
  "name": "portfolio",
  "version": "10.0.0",
  "description": "My personal website.",
  "repository": "https://github.com/mattcarlotta/portfolio",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "LOAD_CONFIG=prod NODE_OPTIONS='-r @noshot/env' next build",
    "build:stage": "LOAD_CONFIG=stage NODE_OPTIONS='-r @noshot/env' next build",
    "dev": "LOAD_CONFIG=dev NODE_OPTIONS='--max_old_space_size=4096 -r @noshot/env' next dev",
    "lint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./src --ext .ts,.tsx",
    "stage": "LOAD_CONFIG=stage NODE_OPTIONS='-r @noshot/env' next start",
    "start": "LOAD_CONFIG=prod NODE_OPTIONS='-r @noshot/env' next start",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --coverage",
    "test:failed": "jest --watch --onlyFailures",
    "test:prod": "npm run build:prod && npm run start",
    "test:stage": "npm run build:stage && npm run stage",
    "test:watch": "jest --watch --onlyChanged",
    "test:watchall": "jest --watchAll",
    "types": "tsc --p ./tsconfig.json"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^4.12.3",
    "@noshot/env": "^2.1.1",
    "@react-icons/all-files": "^4.1.0",
    "chalk": "^4.1.2",
    "dayjs": "^1.10.7",
    "framer-motion": "^5.3.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "next": "^12.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@emotion/jest": "^11.6.0",
    "@noshot/utils": "^1.0.1",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^27.0.2",
    "@types/jsdom": "^16.2.13",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isempty": "^4.4.6",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "browserslist": "^4.18.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-watch": "^8.0.0",
    "jest": "^27.3.1",
    "jest-emotion": "^11.0.0",
    "jest-enzyme": "^7.1.2",
    "jest-haste-map": "^27.3.1",
    "jest-resolve": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^18.1.0",
    "opener": "^1.5.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.1"
  }
}
