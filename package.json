{
  "name": "portoflio",
  "version": "2.1.0",
  "description": "My personal website.",
  "repository": "https://github.com/mattcarlotta/portfolio",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": false,
  "scripts": {
    "analyze:prod": "rimraf .next/analyze && analyze=true npm run build",
    "analyze:stage": "rimraf .next/analyze && analyze=true npm run build:stage",
    "build": "LOAD_CONFIG=prod inStaging=false NODE_OPTIONS='-r snackables' next build",
    "build:stage": "LOAD_CONFIG=prod inStaging=true NODE_OPTIONS='-r snackables' next build",
    "dev": "LOAD_CONFIG=dev NODE_OPTIONS='--max_old_space_size=4096 -r snackables' next dev",
    "export": "next export",
    "lint": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./src --ext .ts,.tsx",
    "stage": "LOAD_CONFIG=prod NODE_OPTIONS='-r snackables' inStaging=true next start -p 8080",
    "start": "LOAD_CONFIG=prod NODE_OPTIONS='-r snackables' next start -p 8080",
    "test": "jest --config ./jest.json --passWithNoTests",
    "test:cov": "jest --config ./jest.json --coverage",
    "test:e2e": "cypress open --project ./e2e",
    "test:e2erun": "npm run build:stage && concurrently --success first --kill-others \"npm run start\" \"cypress run --config video=false --project ./e2e\"",
    "test:failed": "jest --config ./jest.json --watch --onlyFailures",
    "test:prod": "npm run build:prod && npm run start",
    "test:stage": "npm run build:stage && npm run stage",
    "test:watch": "jest --config ./jest.json --watch --onlyChanged",
    "test:watchall": "jest --config ./jest.json .-watchAll",
    "tsc": "tsc --p ./tsconfig.json"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@react-icons/all-files": "^4.1.0",
    "chalk": "^4.1.1",
    "dayjs": "^1.10.5",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "next": "^10.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "snackables": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@emotion/jest": "^11.3.0",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.23",
    "@types/jsdom": "^16.2.10",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isempty": "^4.4.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "address": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "browserslist": "^4.16.6",
    "concurrently": "^6.2.0",
    "cypress": "^7.4.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-watch": "^7.0.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-css-modules-transform": "^4.2.1",
    "jest-emotion": "^11.0.0",
    "jest-enzyme": "^7.1.2",
    "jest-haste-map": "^26.6.2",
    "jest-resolve": "^26.6.2",
    "jsdom": "^16.6.0",
    "opener": "^1.5.2",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
